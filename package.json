{
  "name": "gatsby-starter-nova",
  "description": "Nova is a gatsby starter made for quickly and easily launching SEO-friendly sites.",
  "version": "1.0.0",
  "private": true,
  "author": "With Pulp <hello@withpulp.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withpulp/gatsby-starter-nova"
  },
  "bugs": {
    "url": "https://github.com/withpulp/gatsby-starter-nova/issues"
  },
  "keywords": [
    "gatsby",
    "mdx",
    "nova",
    "purgecss",
    "react",
    "starter",
    "tailwind",
    "typescript",
    "withpulp"
  ],
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "dev:netlify": "netlify dev",
    "fix": "prettier --write  \"**/*.{js,jsx,json,md,mdx,mjs,ts,tsx,yml}\"",
    "purge": "rm -rf .cache node_modules public yarn.lock",
    "serve": "gatsby serve",
    "start": "yarn dev",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "classnames": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-helmet-async": "latest"
  },
  "devDependencies": {
    "@mdx-js/mdx": "latest",
    "@mdx-js/react": "latest",
    "@types/classnames": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-helmet-async": "latest",
    "cssnano": "latest",
    "gatsby": "latest",
    "gatsby-image": "latest",
    "gatsby-plugin-advanced-sitemap": "latest",
    "gatsby-plugin-favicon": "latest",
    "gatsby-plugin-google-gtag": "latest",
    "gatsby-plugin-manifest": "latest",
    "gatsby-plugin-mdx": "latest",
    "gatsby-plugin-next-seo": "latest",
    "gatsby-plugin-offline": "latest",
    "gatsby-plugin-postcss": "^2.3.13",
    "gatsby-plugin-root-import": "latest",
    "gatsby-plugin-sharp": "latest",
    "gatsby-plugin-typescript": "latest",
    "gatsby-remark-copy-linked-files": "latest",
    "gatsby-remark-prismjs": "latest",
    "gatsby-remark-images": "latest",
    "gatsby-source-filesystem": "latest",
    "gatsby-transformer-sharp": "latest",
    "gatsby-transformer-yaml": "latest",
    "postcss-import": "latest",
    "postcss-preset-env": "latest",
    "prettier": "latest",
    "remark-emoji": "latest",
    "remark-slug": "latest",
    "tailwindcss": "latest",
    "typescript": "latest"
  }
}
